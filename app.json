{
  "expo": {
    "name": "Fooodz scanner",
    "slug": "Fooodz_scanner",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "sdkVersion" : "38.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode" : "contain",
      "backgroundColor": "#FA575A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.suTron.Fooodz",
      "enableDangerousExperimentalLeanBuilds" : true
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
